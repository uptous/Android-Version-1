<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/_19sdp"
        android:src="@android:drawable/ic_menu_compass"
        android:visibility="gone"
        app:backgroundTint="#5284e0"
        app:fabSize="mini" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/_9sdp"
        android:background="@drawable/fab"
        android:src="@mipmap/mm"
        android:visibility="invisible"
        app:backgroundTint="#90000000"
        app:borderWidth="0dp"
        app:elevation="0dp"
        app:fabSize="mini"
        app:pressedTranslationZ="@dimen/_12sdp"
        app:rippleColor="#00000000" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/_9sdp"
        android:background="@drawable/fab"
        android:src="@mipmap/pp"
        android:visibility="invisible"
        app:backgroundTint="#90000000"
        app:borderWidth="0dp"
        app:elevation="0dp"
        app:fabSize="mini"
        app:pressedTranslationZ="@dimen/_12sdp"
        app:rippleColor="#90000000" />
</FrameLayout>